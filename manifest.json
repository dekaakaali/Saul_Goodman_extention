{
    "manifest_version": 3,
    "name": "saul is everywhere",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "Popup.html",
        "default_title": "Saul Goodman"
    }
}